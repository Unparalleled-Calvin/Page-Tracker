{
    "manifest_version": 3,
    "name": "Visual History",
    "version": "1.0",
    "description": "A plug-in providing friendly & visual browsing history on Edge & Chrome.",
    "icons": {},
    "permissions": [
        "tabs",
        "storage",
        "webNavigation",
        "history"
    ],
    "action": {
        "default_popup": "./html/popup.html"
    },
    "background": {
        "service_worker": "./js/background.js"
    }
}